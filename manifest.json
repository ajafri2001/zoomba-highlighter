{
  "manifest_version": 3,
  "name": "Zoomba Highlighter",
  "description": "Zoomba Syntax Highlighter for github",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "css": [
        "base16-dark.css"
      ],
      "js": [
        "codemirror-lib.js",
        "javascript/javascript.js",
        "CodeMirror.js"
      ]
    }
  ]
}